{
  "name": "nuxtcommerce",
  "version": "3.1.22",
  "private": true,
  "author": "zackha",
  "description": "Headless storefront for Woocommerce (Nuxt/Vue)",
  "type": "module",
  "scripts": {
    "build": "nuxi build --preset=cloudflare_pages",
    "dev": "nuxt dev",
    "dev:ssl": "nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "deploy:pages": "bumpp && pnpm build && wrangler pages deploy dist --project-name commerce"
  },
  "dependencies": {
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^2.22.3",
    "@nuxtjs/i18n": "10.1.0",
    "graphql-request": "^7.4.0",
    "notivue": "^2.4.5",
    "nuxt": "^4.3.1",
    "swiper": "^12.1.1",
    "vue": "latest"
  },
  "devDependencies": {
    "@iconify-json/iconamoon": "^1.2.2",
    "@iconify-json/svg-spinners": "^1.2.4",
    "@vueuse/core": "^13.9.0",
    "@vueuse/nuxt": "^13.9.0",
    "wrangler": "^4.66.0"
  },
  "packageManager": "pnpm@10.30.0+sha512.2b5753de015d480eeb88f5b5b61e0051f05b4301808a82ec8b840c9d2adf7748eb352c83f5c1593ca703ff1017295bc3fdd3119abb9686efc96b9fcb18200937"
}
